{
  "name": "pudding_bot",
  "version": "1.5.0-alpha",
  "description": "PuddingBot serves the discord server Puddings",
  "scripts": {
    "build": "tsc --project ./",
    "start": "npx ts-node src/index.ts",
    "startbuild": "node build/index.js",
    "dev": "concurrently --raw \"tsx watch src/index.ts\" \"tsc --noEmit --watch\""
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^1.7.0",
    "@discordjs/voice": "^0.15.0",
    "@xmldom/xmldom": "^0.9.5",
    "croner": "^9.0.0",
    "discord-api-types": "^0.37.38",
    "discord-player": "^6.1.1",
    "discord.js": "^14.9.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
