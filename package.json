{
  "name": "pudding_bot",
  "version": "1.5.0",
  "type": "module",
  "description": "PuddingBot serves the discord server Puddings",
  "private": true,
  "scripts": {
    "build": "tsc --project ./",
    "start": "tsx --env-file=.env src/index.ts",
    "startbuild": "node --env-file=.env build/index.js",
    "dev": "concurrently --raw \"tsx watch --env-file=.env src/index.ts\" \"tsc --noEmit --watch\""
  },
  "dependencies": {
    "@discord-player/extractor": "^7.1.0",
    "@discordjs/voice": "^0.19.0",
    "@xmldom/xmldom": "^0.9.5",
    "croner": "^9.1.0",
    "discord-player": "^7.1.0",
    "discord.js": "^14.22.1",
    "mediaplex": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.5.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "license": "GPL-3.0-or-later"
}
